#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/pino-pretty@4.5.0/node_modules/pino-pretty/node_modules:/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/pino-pretty@4.5.0/node_modules:/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/pino-pretty@4.5.0/node_modules/pino-pretty/node_modules:/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/pino-pretty@4.5.0/node_modules:/Users/yasnielfajardo/Documents/PROYECTOS/CENCOSUD/poc-registry-github/npm-js-bit-lib/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pino-pretty@4.5.0/node_modules/pino-pretty/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pino-pretty@4.5.0/node_modules/pino-pretty/bin.js" "$@"
fi
